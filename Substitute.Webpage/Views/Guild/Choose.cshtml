@model IEnumerable<Substitute.Business.DataStructs.Guild.UserGuildModel>

@{
    ViewData["Title"] = "Choose";
}

<h2>Choose server</h2>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4 col-lg-3 col-sm-6 col-xs-12 text-center">
            <a class="thumbnail" href="@Url.Action("Choose", new { guildId = item.Id })">
                <img src="@item.IconUrl" alt="" />
                <div class="caption">
                    <p>@item.Name</p>
                </div>
            </a>
        </div>
    }
</div>